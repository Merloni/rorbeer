<ul>
 <% @ratings.each do |rating| %>
   <li> 
   		<%= rating.to_s %>
   		<i> Made by <%= link_to rating.user.username, rating.user %> </i>
   	</li>
 <% end %>
</ul>
<p>
	<% if @ratings.empty? %>
	No ratings yet!
	<% end %>

</p>


<% if current_user %>
<%= link_to 'New rating', new_rating_path %>
<% end %>